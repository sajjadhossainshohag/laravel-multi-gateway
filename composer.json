{
  "name": "paymentsetu/paybridge",
  "description": "Multi-Gateway Supports in One Pack",
  "type": "library",
  "require": {
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PaymentSetu\\PayBridge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Sajjad Hossain Shohag",
      "email": "sajjadhossainshohagbd@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "pestphp/pest": "^4.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
